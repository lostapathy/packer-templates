{
  "builders": [
    {
      "type": "virtualbox-ovf",
      "source_path": "build/ubuntu64-ruby-virtualbox/ubuntu64-ruby.ovf",
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "ssh_wait_timeout": "30s",
      "shutdown_command": "echo 'vagrant' | sudo -S shutdown -P now",
      "vm_name": "ubuntu64-ruby-passenger",
      "output_directory": "build/ubuntu64-ruby-passenger-virtualbox"
    },
    {
      "type": "digitalocean",
      "image": "ubuntu64-ruby",
      "region": "nyc2",
      "size": "1gb",
      "snapshot_name": "ubuntu64-ruby-passenger",
      "state_timeout": "15m"
    },
    {
      "type": "amazon-ebs",
      "region": "us-east-1",
      "source_ami": "ami-06067b6c",
      "instance_type": "m3.medium",
      "ssh_username": "ubuntu",
      "ami_name": "ubuntu64-ruby-passenger"
    }
  ],
  "provisioners": [
    {
      "type": "chef-solo",
      "cookbook_paths": ["cookbooks"],
      "run_list": ["passenger"],
      "json": {
        "passenger": {
          "version": "5.0.23"
        },
        "ruby": {
          "version": "2.3.0"
        }
      }
     }],
  "post-processors": [
  ]
}

